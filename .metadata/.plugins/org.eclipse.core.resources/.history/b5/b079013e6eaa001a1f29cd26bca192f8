package org.Practice12_1_3.eclipse;

public class CountDigit {
	
	int num;
	
	public int countDigits(String s) {
		char a=s.charAt(0);
		String s_copy;
		s_copy=s.substring(a+1);
		
		if(s.length()<=0) {
			return num;
		}
		else if(('0'<=a&&a<='9')) {
			num++;
		}
		else {
			countDigits()
		}

		return countDigits(s_copy);
	}

}
